{
	"version": "2.0.0",
	"tasks": [{
		// "type": "shell",
		// "label": "shell: g++",
		"label": "g++",
		"command": "g++.exe",
		"args": [
			"-g",
			"${file}",
			"-include",
			"${workspaceRoot}\\.vscode\\head.h",
			"-o",
			"${fileDirname}\\${fileBasenameNoExtension}.exe"
		],
		"options": {
			"cwd": "C:\\MinGW\\bin"
		},
		"problemMatcher": [
			"$gcc"
		],
		"group": "build"
	}]
}